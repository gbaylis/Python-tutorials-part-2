class person(object):
    population = 50
    def __init__(self, name, age):
        self.name = name
        self.age = age
        
    @classmethod
    def getPopulation(cls):
        return cls.population 
        
    @staticmethod
    def isAdult(age):
        return age >= 18
        
    def display(self):
        print(self.name, self.age)
        
Person1 = person('Rusty', 20)
print(person.isAdult(26)) # output: True
# True, because 26 is older then 18
